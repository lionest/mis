<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"    "http://struts.apache.org/dtds/struts-2.1.7.dtd"><struts>	<!-- 不需要登录的请求 -->	<package name="admin-base" extends="public-base" namespace="/admin/base">			<action name="login" method="login" class="com.htlab.mis.web.action.UserAction">		</action>		<action name="logout" method="logout" class="com.htlab.mis.web.action.UserAction">			<result>/pages/admin/login.jsp</result>		</action>				<action name="checkCode" method="checkCode" class="com.htlab.mis.web.action.CheckCodeImageAction">		</action>		<action name="keUpload"			class="com.htlab.mis.web.action.KinderEditorAction"			method="upload">			<result type="json">				<param name="root">resultMap</param>				<param name="contentType">text/html</param>			</result>		</action>		<action name="keFileManager"			class="com.htlab.mis.web.action.KinderEditorAction"			method="fileManager">		</action>			</package>		<!-- 需要登录的请求 -->	<package name="admin_private" extends="private-base" namespace="/admin/pri">		<action name="mysqldb_*" method="{1}" class="com.htlab.mis.web.action.MysqlBackAction">			<result name="list">/pages/admin/pri/user/list.jsp</result>			<result name="edit">/pages/admin/pri/user/add.jsp</result>			<result name="doList" type="redirectAction">				<param name="actionName">user_list.action</param>				<param name="pageIndex">${pageIndex}</param>			</result>		</action>		<action name="user_*" method="{1}" class="com.htlab.mis.web.action.UserAction">			<result name="list">/pages/admin/pri/user/list.jsp</result>			<result name="edit">/pages/admin/pri/user/add.jsp</result>			<result name="doList" type="redirectAction">				<param name="actionName">user_list.action</param>				<param name="pageIndex">${pageIndex}</param>			</result>		</action>		<action name="supplier_*" method="{1}" class="com.htlab.mis.web.action.SupplierAction">			<result name="list">/pages/admin/pri/supplier/list.jsp</result>			<result name="import">/pages/admin/pri/supplier/import.jsp</result>			<result name="edit">/pages/admin/pri/supplier/add.jsp</result>			<result name="doList" type="redirectAction">				<param name="actionName">supplier_list.action</param>				<param name="pageIndex">${pageIndex}</param>			</result>		</action>		<action name="role_*" method="{1}" class="com.htlab.mis.web.action.RoleAction">			<result name="list">/pages/admin/pri/role/list.jsp</result>			<result name="edit">/pages/admin/pri/role/add.jsp</result>			<result name="doList" type="redirectAction">				<param name="actionName">role_list.action</param>				<param name="pageIndex">${pageIndex}</param>			</result>		</action>						<action name="irr_*" method="{1}" class="com.htlab.mis.web.action.IrrAction">			<result name="list">/pages/admin/pri/irr/list.jsp</result>			<result name="listReport">/pages/admin/pri/irr/listReport.jsp</result>			<result name="edit">/pages/admin/pri/irr/add.jsp</result>				<result name="time">/pages/admin/pri/irr/time.jsp</result>			<result name="doList" type="redirectAction">				<param name="actionName">irr_list.action</param>				<param name="pageIndex">${pageIndex}</param>			</result>					</action>		<action name="irrfile_*" method="{1}" class="com.htlab.mis.web.action.IrrFileAction">			<result name="edit">/pages/admin/pri/irrFile/add.jsp</result>		</action>		<action name="chart_*" method="{1}" class="com.htlab.mis.web.action.ChartAction">			<result name="success">/pages/admin/pri/chart/list.jsp</result>		</action>		<action name="config_*" method="{1}" class="com.htlab.mis.web.action.GlobalConfigAction">			<result name="list">/pages/admin/pri/config/list.jsp</result>		</action>					<action name="part_*" method="{1}" class="com.htlab.mis.web.action.PartAction">			<result name="list">/pages/admin/pri/part/list.jsp</result>			<result name="edit">/pages/admin/pri/part/add.jsp</result>			<result name="import">/pages/admin/pri/part/import.jsp</result>			<result name="doList" type="redirectAction">				<param name="actionName">part_list.action</param>				<param name="pageIndex">${pageIndex}</param>			</result>		</action>				<action name="clean" method="clean" class="com.htlab.mis.web.action.SystemAction">					</action>	</package>	</struts>